   "source": [
    "import glob\n",
    "\n",
    "file_list = glob.glob('*txt')\n",
    "\n",
    "with open('merge.txt', 'w') as outfile:\n",
    "    for filename in sorted(file_list):\n",
    "        with open(filename) as file:\n",
    "            for line in file:\n",
    "                outfile.write(line)"
   ]
  }
